<!DOCTYPE html>
<html>
  <head>
    <title>EF/Camp Help - <%= breadcrumb[breadcrumb.length-1].anchor %></title>
    <meta name="viewport" content="width=device-width, initial-scale=1, maximum-scale=1, user-scalable=no">    
    <link rel="SHORTCUT ICON" href="http://media.ef.com/favicon.ico" />
    <% if (lang == "zh") { %>
    <link rel="stylesheet" type="text/css" href="http://libs.baidu.com/bootstrap/3.0.3/css/bootstrap.css">
    <% } else {%>
    <link rel="stylesheet" type="text/css" href="//maxcdn.bootstrapcdn.com/bootstrap/3.3.1/css/bootstrap.min.css">
    <% } %>
    <link rel="stylesheet" href="/stylesheets/style.css">
  </head>
  <body>
    <div class="panel-container">
      <div class="logo">
        <img src="/images/logo.png">
      </div>
      <div class="panel-box container">
        <div class="row">
          <div class="col-md-12 title-container">
            <span>
              <%= breadcrumb[breadcrumb.length-1].anchor %>
            </span>
          </div>
        </div>
        <div class="row">
          <div class="col-md-12">
            <ol class="breadcrumb">
              <% breadcrumb.forEach(function(bc){ %>
                <li class="<%= bc.isCurrent ? 'active' : ''%>">
                  <% if (bc.isCurrent) { %>
                  <%=  bc.anchor %>
                  <% } else { %>
                  <a href="<%= bc.href %>"><%= bc.anchor %></a>
                  <%}%>
                </li>
              <% }) %>
            </ol>
          </div>
        </div>
        <div class="main">
          <%- body %>  
        </div>
      </div>
      <div class="footer text-center">
      &copy EF Education First. <a href="/en/">English</a> | <a href="/zh/">中文</a> 
      </div>
    </div>
    <script>
      (function(i,s,o,g,r,a,m){i['GoogleAnalyticsObject']=r;i[r]=i[r]||function(){
      (i[r].q=i[r].q||[]).push(arguments)},i[r].l=1*new Date();a=s.createElement(o),
      m=s.getElementsByTagName(o)[0];a.async=1;a.src=g;m.parentNode.insertBefore(a,m)
      })(window,document,'script','//www.google-analytics.com/analytics.js','ga');

      ga('create', 'UA-48369305-10', 'auto');
      ga('send', 'pageview');

    </script>

  </body>
</html>
